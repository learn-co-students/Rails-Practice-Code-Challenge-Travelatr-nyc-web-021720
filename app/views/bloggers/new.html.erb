<h1>Create a new Blogger! </h1>

 <%if flash[:errors]%>
    <ul>
     
     <%flash[:errors].each do |error| %>

     <li><%=error%></li>   
    
    </ul>
    <%end%>
 <%end%>

<%= form_for @blogger do |f|%>
   Name <%= f.text_field :name%> <br>
   Age  <%= f.number_field :age%> <br>
   Bio <%= f.text_area :bio %> <br>
   <%=f.submit "Create Blogger"%>
<%end%>